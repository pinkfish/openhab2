<xml>
	<feature name="GenericSwitch" timeout="5000">
		<message-handler cmd="ProductDataRequest" group="1">NoOpMsgHandler</message-handler>
		<message-handler cmd="LightOn" group="1">LightOnSwitchHandler</message-handler>
		<message-handler cmd="LightOnFast" group="1" mode="FAST">LightOnSwitchHandler</message-handler>
		<message-handler cmd="LightOff" group="1">LightOffSwitchHandler</message-handler>
		<message-handler cmd="LightOffFast" group="1" mode="FAST">LightOffSwitchHandler</message-handler>
		<message-handler cmd="LightStatusRequest">SwitchRequestReplyHandler</message-handler>
		<command-handler command="OnOffType">LightOnOffCommandHandler</command-handler>
	</feature>
	<feature name="FastOnOff">
		<message-handler cmd="LightOnFast" mode="FAST">LightOnSwitchHandler</message-handler>
		<message-handler cmd="LightOffFast" mode="FAST">LightOffSwitchHandler</message-handler>
		<command-handler command="OnOffType">FastOnOffCommandHandler</command-handler>
	</feature>

	<feature name="RampDimmer">
		<!-- cmd1 defaults to 0x2E, 0x2F -->
		<!-- default dispatcher uses 0x19 for lookup key instead of cmd1 -->
		<message-handler cmd="LightStatusRequest">RampDimmerHandler</message-handler>
		<command-handler command="PercentType">RampPercentHandler</command-handler>
		<command-handler command="OnOffType">RampOnOffCommandHandler</command-handler>
	</feature>

	<feature name="RampDimmer_3435">
		<!-- default dispatcher uses 0x19 for lookup key instead of cmd1 -->
